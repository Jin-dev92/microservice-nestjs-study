{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/microservice-nestjs/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/microservice-nestjs/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/microservice-nestjs",
  "projects": {
    "microservice-nestjs": {
      "type": "application",
      "root": "apps/microservice-nestjs",
      "entryFile": "main",
      "sourceRoot": "apps/microservice-nestjs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/microservice-nestjs/tsconfig.app.json"
      }
    },
    "upload": {
      "type": "application",
      "root": "apps/upload",
      "entryFile": "main",
      "sourceRoot": "apps/upload/src",
      "compilerOptions": {
        "tsConfigPath": "apps/upload/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "rmq": {
      "type": "library",
      "root": "libs/rmq",
      "entryFile": "index",
      "sourceRoot": "libs/rmq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rmq/tsconfig.lib.json"
      }
    }
  }
}